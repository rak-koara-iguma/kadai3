<div class="container">
  <div class="row">
      <div class="col-md-3" style="background-color: white;">
  <%= render 'users/leftunder', user: @user %>
  <%= render 'books/leftside', books: @books %>
      </div>
<div class="col-md-8 offset-md-1">

<h1>Users</h1>
<table class="table table-hover table-inverse">
  <thead>
  <tr>
    <th>Image</th>
    <th>name</th>
    <th></th>
  </tr>
  </thead>
  <tbody>
<% @users.each do |user| %>
  <tr>
    <th><%= image_tag user.get_profile_image(100,100) %></th>
    <th><%=link_to user.name, user_path(user.id) %></th>
    <th><%= link_to "Show", user_path(user.id) %></th>
  </tr>
<% end %>
</tbody>
</table>
</div>
</div>
</div>
